{
  "expo": {
    "name": "app-masaltos",
    "slug": "app-masaltos",
    "scheme": "masaltosapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo-masaltos.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo-masaltos.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "permissions": [
          "INTERNET",
          "ACCESS_NETWORK_STATE",
          "INTERNET",
          "ACCESS_NETWORK_STATE"
        ],
        "foregroundImage": "./assets/logo-masaltos.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.masaltos.dev"
    },
    "web": {
      "favicon": "./assets/logo-masaltos.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
